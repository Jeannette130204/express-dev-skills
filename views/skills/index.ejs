<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Express skills</title>
</head>
<body>
    <h1>My skills</h1>
    <ul>
      <% skills.forEach(function(skill) { %>
        <li>
          <a href="/skills/<%= skill.id %>"><%= skill.skill %></a>
          <form action="/skills/<%=skill.id %>?_method=DELETE" class="delete-form" method="POST">
            <button type="submit">Delete</button></form>
            
        </li>
      <% }); %>
    </ul>
    <a href="/skills/new">Add new dev skill</a>
  </body>


</html>